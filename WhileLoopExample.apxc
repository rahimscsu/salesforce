public class WhileLoopExample {

    public static void loopWhileDemo() {
        integer i = 1;
        while (i < 10) {
            system.debug(i);
            i++;
        }
    }

    public static void doWhileLoopDemo() {
        integer i = 1;
        do {
            system.debug(i);
            i++;
        }while(i < 10);
    }

}
